<div class="inputWrapper">
  <label>
    {{@filter}}
  </label>
  {{#if this.isTextFilter}}
    <TextFilter @filterName={{@filter}} @addFilter={{@addFilter}}/>
  {{/if}}
  {{#if this.isSelectFilter}}
    <SelectFilter @filterName={{@filter}} @addFilter={{@addFilter}}/>
  {{/if}}
  {{#if this.isRangeFilter}}
    <RangeFilter @filterName={{@filter}} @addFilter={{@addFilter}}/>
  {{/if}}
</div>
