<TableFilters
  @filterableColumns={{this.filterableColumns}}
  @onFilterAdded={{this.onFilterAdded}}
  @sortTable={{this.sortTable}}
/>

<div class="container">
  <table>
    <HoldingHeader @headerData={{@data.columns}} />
    <tbody>
      {{#each this.filteredHoldings as |row|}}
        <HoldingRow @rowData={{row}} />
      {{else}}
        Your filters did not return any results
      {{/each}}
    </tbody>
  </table>
</div>
